{
  "name": "Seamless",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "parameter": {
      "foreground": "#78314f"
    },
    "variable": {
      "foreground": "#984165"
    },
    "property": {
      "foreground": "#822D51" // Your desired color for properties like lineThickness
    },
    "property.declaration": {
      "foreground": "#72435a" // Keeping their current color
    },
    "variable.defaultLibrary": {
      "foreground": "#b63c3c" // Example color for console
    },
    "enumMember": {
      "foreground": "#175f93"
    }
  },
  "tokenColors": [
    {
      "scope": "punctuation.definition.string.begin,punctuation.definition.string.end",
      "settings": {
        "foreground": "#5c7e57"
      }
    },
    {
      "scope": "punctuation.separator.key-value",
      "settings": {
        "foreground": "#c9c9c9"
      }
    },
    {
      "scope": "entity.name.type.module",
      "settings": {
        "foreground": "#37adba"
      }
    },
    {
      "scope": "meta.object-literal.key, meta.objectliteral.js",
      "settings": {
        "foreground": "#B1B1B1"
      }
    },
    {
      "name": "js/ts Keyword",
      "scope": [
        "keyword.operator.expression.instanceof",
        "keyword.operator.new",
        "keyword.operator.ternary",
        "keyword.operator.optional",
        "keyword.operator.expression.keyof"
      ],
      "settings": {
        "foreground": "#386890",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable.other.object.property.js",
      ],
      "settings": {
        "foreground": "#5a5a5a"
      }
    },
    {
      "scope": [
        "variable.other.property.js",
      ],
      "settings": {
        "foreground": "#3b8c6e"
      }
    },
    {
      "scope": "keyword.operator.arithmetic,keyword.operator.comparison,keyword.operator.decrement,keyword.operator.increment,keyword.operator.relational",
      "settings": {
        "foreground": "#755584"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#755584"
      }
    },
    {
      "scope": "keyword.operator.expression.of.js",
      "settings": {
        "foreground": "#8866b2"
      }
    },
    {
      "scope": "keyword.operator.expression.in.js",
      "settings": {
        "foreground": "#9d77cb"
      }
    },
    {
      "name": "Language variables",
      "scope": "variable.language",
      "settings": {
        "foreground": "#455b6e"
      }
    },
    {
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#a55b9f"
      }
    },
    {
      "scope": "storage.type.class",
      "settings": {
        "foreground": "#5d4274"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#3b7b63"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#3b7b63"
      }
    },
    {
      "scope": "token.storage",
      "settings": {
        "foreground": "#3b7b63"
      }
    },
    {
      "scope": "keyword.operator.expression.delete",
      "settings": {
        "foreground": "#401d4a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#71a868"
      }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#a03f4e"
      }
    },
    {
      "name": "Integers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#77a2a2"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#a14d8c"
      }
    },
    {
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#a03f4e"
      }
    },
    {
      "scope": [
        "punctuation.accessor.js"
      ],
      "settings": {
        "foreground": "#c9c9c9"
      }
    },
    {
      "scope": [
        "entity.name.function.js"
      ],
      "settings": {
        "foreground": "#3388af"
      }
    },
  ],
